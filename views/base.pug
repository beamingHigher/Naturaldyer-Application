doctype html
html
  head
    block headDetails
      meta(charset='utf-8')
      meta(name='viewport' content='width=device-width,initial-scale=1,shrink-to-fit=no,user-scalable=0')
      meta(name='description' content='Fabricating technology and Textile Industry by leveraging the use of cryptocurrency and its decentralized economy.')
      meta(name='author' content='WSCF Global')
      title= `Naturaldyers - ${title}`
      //  CSS 
      link(href='css/bootstrap.min.css' rel='stylesheet')
      link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css')
      link(href='css/animsition.min.css' rel='stylesheet')
      link(href='css/owl.carousel.min.css' rel='stylesheet')
      link(href='css/owl.theme.default.min.css' rel='stylesheet')
      link(href='css/socicon.css' rel='stylesheet')
      link(href='css/ionicons.min.css' rel='stylesheet')
      link(href='css/animate.min.css' rel='stylesheet')
      link(href='css/jquery.fancybox.min.css' rel='stylesheet')
      //  Main CSS 
      link(href='css/style.css' rel='stylesheet')
      //  Favicons 
      link(rel='apple-touch-icon' sizes='180x180' href='favicon/apple-touch-icon.png')
      link(rel='icon' type='image/png' sizes='32x32' href='favicon/favicon-32x32.png')
      link(rel='icon' type='image/png' sizes='16x16' href='favicon/favicon-16x16.png')
      link(rel='manifest' href='favicon/site.webmanifest')
      meta(name='msapplication-TileColor' content='#da532c')
      meta(name='theme-color' content='#ffffff')


  body#dashboard.theme-orange.home-flash.dark-horizontal
    .animsition
      .wrapper.parallax-start
        //HEADER
        include header

        // CONTENT
        block content


      //FOOTER
      include footer

    block script
      // Vendor: Javascripts
      script(src='js/bundle.js')
      script(src='https://code.jquery.com/jquery-1.12.4.min.js')
      script(src='js/smoothscroll.js')
      script(src='js/popper.min.js')
      script(src='js/bootstrap.min.js')
      script(src='js/animsition.min.js')
      script(src='js/wow.min.js')
      script(src='js/isotope.pkgd.min.js')
      script(src='js/jquery.fancybox.min.js')
      script(src='js/TweenMax.min.js')
      script(src='js/ScrollMagic.min.js')
      script(src='js/animation.gsap.min.js')
      script(src='js/jquery.viewport.js')
      script(src='js/jquery.countdown.min.js')
